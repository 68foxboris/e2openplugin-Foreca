#: plugin.py:617
msgid ""
"\n"
"0 - 9       =   Prognosis (x) days from now\n"
"\n"
"VOL+/-  =   Fast scroll (City choice)\n"
"\n"
"<   >       =   Prognosis next/previous day\n"
"\n"
"Info        =   This information\n"
"\n"
"Menu     =   Satellite photos and maps\n"
"\n"
"Red        =   Temperature for the next 5 days\n"
"\n"
"Green    =   Go to Favorite 1\n"
"\n"
"Yellow    =   Go to Favorite 2\n"
"\n"
"Blue        =   Go to Home\n"
"\n"
"Wind direction = Arrow to right: Wind from the West\n"
msgstr ""
"\n"
"0 - 9       =   Prognose fuer (x) Tage\n"
"\n"
"VOL+/-  =   Schnell scrollen (Stadtauswahl)\n"
"\n"
"<   >       =   Prognose naechste/vorherige Tag\n"
"\n"
"Info        =   Diese Information\n"
"\n"
"Menue    =   Satellitenfotos und Karten\n"
"\n"
"Rot         =   Temperatur fuer die naechste 5 Tage\n"
"\n"
"Gruen     =   Wechsel nach Favorit 1\n"
"\n"
"Gelb        =   Wechsel nach Favorit 2\n"
"\n"
"Blau        =   Wechsel nach Zuhause\n"
"\n"
"Windrichtung = Pfeil nach rechts: Wind vom Westen\n"

